{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "embed.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/embed.js",
      "dest": "/embed.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
